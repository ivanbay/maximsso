{
    "name": "mxm/sso",
    "type": "library",
    "description": "Maxim SSO Login laravel package",
    "keywords": [
        "mxm",
        "sso"
    ],
    "homepage": "https://github.com/mxm/sso",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Paul Bay",
            "email": "aiipee05@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.4|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "mxm\\sso\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mxm\\sso\\": "tests"
        }
    },
    "minimum-stability": "beta",
    "config": {
        "sort-packages": true
    }
}
